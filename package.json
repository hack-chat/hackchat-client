{
  "name": "hackchat-client",
  "version": "1.0.0",
  "description": "The default client that is webpacked and distributed in the main hack chat repository",
  "repository": {
    "type": "git",
    "url": "git://github.com/hack-chat/hackchat-client.git"
  },
  "engines": {
    "npm": ">=10.8.3",
    "node": ">=22.9.0"
  },
  "author": "Marzavec",
  "license": "WTFPL",
  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color --progress --env production",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint app/**/*.js",
    "lint:eslint": "eslint",
    "lint:eslint:fix": "eslint --fix .",
    "lint:js": "npm run lint:eslint -- ./app ",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "compression": "1.7.5",
    "cross-env": "^7.0.3",
    "dompurify": "^3.1.7",
    "express": "4.21.1",
    "hackchat-engine": "^1.1.5",
    "highlight.js": "^11.10.0",
    "hoist-non-react-statics": "3.3.2",
    "immer": "^10.1.1",
    "intl": "1.2.5",
    "invariant": "2.2.4",
    "katex": "^0.16.11",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "prop-types": "15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "6.1.0",
    "react-icons": "^5.3.0",
    "react-intl": "6.8.7",
    "react-katex": "^3.0.1",
    "react-redux": "9.1.2",
    "react-router-dom": "^6.28.0",
    "reactstrap": "^9.2.3",
    "redux": "5.0.1",
    "redux-saga": "1.3.0",
    "remarkable": "^2.0.1",
    "remarkable-react": "^1.4.3",
    "reselect": "5.1.1",
    "sanitize.css": "13.0.0",
    "styled-components": "6.1.13",
    "use-state-if-mounted": "^1.0.7"
  },
  "overrides": {
    "graceful-fs": "^4.2.11"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.25.9",
    "@babel/plugin-transform-react-constant-elements": "7.25.9",
    "@babel/plugin-transform-react-inline-elements": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.25.9",
    "@eslint/compat": "^1.2.2",
    "@eslint/js": "^9.14.0",
    "@reduxjs/toolkit": "^2.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "babel-loader": "9.2.1",
    "babel-plugin-styled-components": "^2.1.4",
    "circular-dependency-plugin": "5.2.2",
    "compression-webpack-plugin": "^11.1.0",
    "core-js": "^3.39.0",
    "css-loader": "7.1.2",
