{
  "name": "hackchat-client",
  "version": "1.0.0",
  "description": "The default client that is webpacked and distributed in the main hack chat repository",
  "repository": {
    "type": "git",
    "url": "git://github.com/hack-chat/hackchat-client.git"
  },
  "engines": {
    "npm": ">=6",
    "node": ">=14.8.0"
  },
  "author": "Marzavec",
  "license": "WTFPL",
  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color --progress --env production",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint app/**/*.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix .",
    "lint:js": "npm run lint:eslint -- ./app ",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "bootstrap": "^5.0.1",
    "chalk": "^4.1.1",
    "compression": "1.7.4",
    "connected-react-router": "6.9.1",
    "cross-env": "^7.0.3",
    "dompurify": "^2.2.9",
    "express": "4.17.1",
    "fontfaceobserver": "2.1.0",
    "hackchat-engine": "^1.1.2",
    "highlight.js": "^11.0.1",
    "history": "^4.10.1",
    "hoist-non-react-statics": "3.3.2",
    "immer": "9.0.3",
    "install": "^0.13.0",
    "intl": "1.2.5",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "npm": "^7.18.1",
    "prop-types": "15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "6.1.0",
    "react-icons": "^4.2.0",
    "react-intl": "5.20.3",
    "react-redux": "7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.9.0",
    "redux": "4.1.0",
    "redux-saga": "1.1.3",
    "remarkable": "^2.0.1",
    "remarkable-react": "^1.4.3",
    "reselect": "4.0.0",
    "sanitize.css": "12.0.1",
    "styled-components": "5.3.0",
    "use-state-if-mounted": "^1.0.4"
  },
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
